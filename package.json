{
  "name": "chatseller-widget",
  "version": "1.0.0",
  "description": "Embeddable Vue.js widget for ChatSeller - AI Commercial Agent",
  "main": "dist/chatseller.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:lib": "vue-tsc && vite build --mode library",
    "build:vercel": "npm run build:lib && cp index.production.html dist/index.html",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "@vueuse/core": "^10.7.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "terser": "^5.27.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27"
  },
  "keywords": [
    "chatseller",
    "widget",
    "vue",
    "ecommerce",
    "ai",
    "chat",
    "embeddable"
  ],
  "author": "Dukka Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dukka-ChatSeller/chatseller-widget.git"
  }
}
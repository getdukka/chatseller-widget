{
  "name": "chatseller-widget",
  "version": "1.0.0",
  "description": "Widget embeddable d'agent IA commercial pour e-commerce",
  "main": "dist/chatseller.min.js",
  "module": "dist/chatseller.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "npm run type-check && vite build",
    "build:prod": "npm run type-check && vite build --mode production",
    "build:watch": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "echo \"Tests à implémenter\" && exit 0",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "serve": "npx http-server dist -p 8080 -c-1"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "http-server": "^14.1.1",
    "prettier": "^3.1.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  },
  "keywords": [
    "chatbot",
    "e-commerce",
    "ai",
    "widget",
    "shopify",
    "woocommerce",
    "conversion",
    "sales"
  ],
  "author": "ChatSeller Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/getdukka/chatseller-widget.git"
  },
  "bugs": {
    "url": "https://github.com/getdukka/chatseller-widget/issues"
  },
  "homepage": "https://chatseller.app",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}